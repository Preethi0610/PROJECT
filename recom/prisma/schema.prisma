generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model users {
  id       Int     @id(map: "PK__users__3213E83F1BF5AA90") @default(autoincrement())
  name     String? @db.VarChar(Max)
  email    String? @db.VarChar(Max)
  password String? @db.VarChar(Max)
}

